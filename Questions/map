# Write Python 3 code in this online editor and run 
#num=(input("enter the number ").split(" "))
num=tuple(map(int,input("enter the number ").split(" ")))
w=(len(num))
even=0
odd=0
for i in range(w):
    w=int((num[i]))
    if w%2==0:
        even=even+w
    if w%2!=0:
        odd=odd+w
print("sum of even is ",even)
print("sum of odd is " ,odd)
        
