a=int(input("ENTER THE YEAR TO CKECK :  "))
while a<0:
    print("ENTER THE VALID YEAR TO CHECK")
    a=(input())
    break
while a>0:
    b= a%4
    c=a%100
    d=a%400
    if b==0 and c!=0:
        print("YEAR IS LEAD YEAR")
    elif c==0 and d!=0:
         print("YEAR IS LEAD YEAR")
    else:
         print("YEAR IS not LEAD YEAR")
        
    print("ENTER THE VALID YEAR TO CHECK")
    a=int(input())   

