print("Try programiz.pro")
a=int(input())
b=int(input())
c=int(input())
if a <b:
    if a<c:
        print(a, 'is smaller')
if b <c:
    if b<a:
        print(b, "is smaller")
if c <a:
    if c<b:
        print(c, "is smaller")
